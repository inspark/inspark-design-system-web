  "devDependencies": {
    "@csstools/postcss-sass": "^4.0.0",
    "autoprefixer": "^9.6.1",
    "css-loader": "^3.1.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
